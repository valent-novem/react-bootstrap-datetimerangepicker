{
  "name": "react-bootstrap-datetimerangepicker",
  "version": "2.0.4",
  "description": "React date & time range picker component base on bootstrap-daterangepicker. This date range picker component for Bootstrap creates a dropdown menu from which a user can select a range of dates. Features include limiting the selectable date range, localizable strings and date formats, a single date picker mode, optional time picker",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel ./src -d lib",
    "prepublish": "npm run build",
    "gh-pages": "gulp publish:examples",
    "lint": "eslint src examples/src",
    "start": "gulp dev",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luqin/react-bootstrap-datetimerangepicker"
  },
  "license": "ISC",
  "peerDependencies": {
    "react": ">=0.14.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.3",
    "prop-types": "^15.5.9"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "3.3.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "keywords": [
    "react",
    "react-dom",
    "ui",
    "react-component",
    "bootstrap",
    "date",
    "range",
    "picker",
    "selector",
    "datetime"
  ]
}
